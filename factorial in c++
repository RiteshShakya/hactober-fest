#include <iostream>
using namespace std;
int main()
{
    unsigned int n;
    int X;
    char ch;
    unsigned long long factorial = 1;
    x:
   cout << "Enter a integer positive: ";
    cin >> n;
    for(int i = 1; i<=n; ++i)
    {
        factorial *= i;
    }
    cout << "Factorial of " << n << " = " << factorial;    
    cout<<" do you want to continue (y/n) ";
    cin>>ch;
if (ch=='y' || ch=='Y')
goto x;
return 0;
}
